{
  "version": 2,
  "name": "nia-learning-assistant",
  "builds": [
    {
      "src": "demo/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/demo/$1"
    }
  ],
  "headers": [
    {
      "source": "/demo/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
